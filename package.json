{
    "author": "TweenRex",
    "bugs": {
        "url": "https://github.com/tweenrex/tweenrex/issues"
    },
    "description": "Bundled CDN files for TweenRex",
    "devDependencies": {
        "@types/node": "^8.0.53",
        "del-cli": "^1.1.0",
        "rollup": "^0.51.8",
        "rollup-plugin-commonjs": "^8.2.6",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-typescript": "^0.8.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.2",
        "uglify-js": "^3.2.0"
    },
    "homepage": "https://github.com/tweenrex/tweenrex#readme",
    "license": "MIT",
    "name": "tweenrex",
    "repository": {
        "type": "git",
        "url": "https://github.com/tweenrex/tweenrex"
    },
    "types": "./types/index",
    "typings": "./types/index",
    "scripts": {
        "build": "npm run build:core && npm run build:debug && npm run build:render && npm run build:all ",
        "build:all": "rollup -c ./config/rollup.all.js && uglifyjs --config-file ./config/compress.json -o dist/tweenrex-all.min.js dist/tweenrex-all.js",
        "build:core": "rollup -c ./config/rollup.core.js && uglifyjs --config-file ./config/compress.json -o dist/tweenrex.min.js dist/tweenrex.js",
        "build:debug": "rollup -c ./config/rollup.debug.js && uglifyjs --config-file ./config/compress.json -o dist/tweenrex-debug.min.js dist/tweenrex-debug.js",
        "build:render": "rollup -c ./config/rollup.render.js && uglifyjs --config-file ./config/compress.json -o dist/tweenrex-render.min.js dist/tweenrex-render.js",
        "clean": "node_modules/.bin/del-cli -f dist",
        "postversion": "git push --follow-tags && npm publish",
        "preversion": "npm run rebuild",
        "rebuild": "npm run clean && npm run build"
    },
    "version": "1.3.0",
    "dependencies": {
        "@tweenrex/core": "1.3.1",
        "@tweenrex/render": "1.2.5",
        "@tweenrex/debug": "0.0.6"
    }
}
